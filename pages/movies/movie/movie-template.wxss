@import '../star/star-template.wxss';
.movie-container{
  display:flex;
  flex-direction:column;
  padding:0 22rpx 10rpx;
}
.movie-container image{
  width:200rpx;
  height:270rpx;
  padding-bottom:20rpx;
}
.movie-container > text{
  padding-bottom:16rpx;
  width:200rpx;
  text-overflow:ellipsis;
  white-space: nowrap;
  overflow: hidden;
}